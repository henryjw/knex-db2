{
  "name": "knex-db2",
  "version": "2.0.0",
  "description": "Knex dialect for DB2",
  "main": "src/index.js",
  "engines": {
    "node": ">=6"
  },
  "engineStrict": true,
  "scripts": {
    "test": "mocha test/unit && mocha --recursive test/integration",
    "test-min-knex": "npm i knex@0.15.2 && npm test",
    "test-latest-knex": "npm i knex@* && npm test",
    "lint": "eslint '{src,test}/**/*.js'"
  },
  "keywords": [
    "knex",
    "dialect",
    "querybuilder",
    "db2"
  ],
  "author": {
    "name": "Henry Williams",
    "web": "https://github.com/henryjw"
  },
  "repository": {
    "type": "git",
    "url": "git://github.com/henryjw/knex-db2.git"
  },
  "license": "MIT",
  "devDependencies": {
    "babel-eslint": "^10.0.1",
    "chai": "^4.2.0",
    "eslint": "^5.6.1",
    "eslint-config-airbnb-base": "^13.1.0",
    "eslint-config-defaults": "^9.0.0",
    "eslint-plugin-import": "^2.14.0",
    "eslint-plugin-react": "^7.11.1",
    "husky": "^1.1.2",
    "knex": "^0.16.3",
    "mocha": "^5.2.0",
    "odbc": "^2.2.2"
  },
  "dependencies": {
    "bluebird": "^3.5.2"
  },
  "files": [
    "src/*",
    "CHANGELOG.md",
    "README.md",
    "LICENSE"
  ],
  "husky": {
    "hooks": {
      "pre-commit": "npm run lint",
      "pre-push": "npm test"
    }
  }
}
